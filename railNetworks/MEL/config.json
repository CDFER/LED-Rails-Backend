{
  "GTFSRealtimeAPI": {
    "url": ["https://api.opendata.transport.vic.gov.au/opendata/public-transport/gtfs/realtime/v1/metro/vehicle-positions",
            "https://api.opendata.transport.vic.gov.au/opendata/public-transport/gtfs/realtime/v1/vline/vehicle-positions"],
    "tripsUrl": ["https://api.opendata.transport.vic.gov.au/opendata/public-transport/gtfs/realtime/v1/metro/trip-updates",
                 "https://api.opendata.transport.vic.gov.au/opendata/public-transport/gtfs/realtime/v1/vline/trip-updates"],
    "keyHeader": "KeyId",
    "fetchIntervalSeconds": 15,
    "format": "GTFSRealtime",
    "protocol": "protobuf"
  },
  "processingOptions": {
    "pairTrains": false,
    "mapStopsToPlatforms": true,
    "displayThreshold": 240,
    "removeStaleVehiclesHours": 1,
    "cacheGTFS": true,
    "cacheIntervalSeconds": 360
  },
  "trainFilter": {
    "trip_ID": {
      "excludes": ["ABY"]
    }
  },
  "stops": {
    "fileName": "stops.txt"
  },
  "trackBlocks": {
    "fileName": "trackBlocks.kml"
  },
