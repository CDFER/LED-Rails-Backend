{
  "GTFSRealtimeAPI": {
    "url": ["https://api.opendata.transport.vic.gov.au/opendata/public-transport/gtfs/realtime/v1/metro/vehicle-positions",
            "https://api.opendata.transport.vic.gov.au/opendata/public-transport/gtfs/realtime/v1/vline/vehicle-positions"],
    "tripsUrl": ["https://api.opendata.transport.vic.gov.au/opendata/public-transport/gtfs/realtime/v1/metro/trip-updates",
                 "https://api.opendata.transport.vic.gov.au/opendata/public-transport/gtfs/realtime/v1/vline/trip-updates"],
    "keyHeader": "KeyId",
    "fetchIntervalSeconds": 30,
    "format": "GTFSRealtime",
    "protocol": "protobuf"
  },
  "processingOptions": {
    "pairTrains": false,
    "mapStopsToPlatforms": true,
    "displayThreshold": 240,
    "removeStaleVehiclesHours": 1,
    "cacheGTFS": true,
    "cacheIntervalSeconds": 360
  },
  "trainFilter": {
    "trip_ID": {
      "excludes": ["ABY"]
    }
  },
  "stops": {
    "fileName": "stops.txt"
  },
  "trackBlocks": {
    "fileName": "trackBlocks.kml"
  },
  "LEDRailsAPI": {
    "APIVersions": [{ "version": "100" }],
    "randomizeTimeOffset": true,
    "colors": {
      "OUT-OF-SERVICE": [255, 255, 255],
      "aus:vic:vic-02-UFD:": [255, 128, 0],
      "aus:vic:vic-02-CGB:": [255, 128, 0],
      "aus:vic:vic-02-WIL:": [255, 0, 128],
      "aus:vic:vic-02-WER:": [255, 0, 128],
      "aus:vic:vic-02-SHM:": [255, 0, 128],
      "aus:vic:vic-02-MDD:": [255, 0, 0],
      "aus:vic:vic-02-HBE:": [255, 0, 0],
      "aus:vic:vic-02-LIL:": [0, 0, 255],
      "aus:vic:vic-02-BEG:": [0, 0, 255],
      "aus:vic:vic-02-ALM:": [0, 0, 255],
      "aus:vic:vic-02-GWY:": [0, 0, 255],
      "aus:vic:vic-02-SUY:": [0, 128, 128],
      "aus:vic:vic-02-PKM:": [0, 128, 128],
      "aus:vic:vic-02-CBE:": [0, 128, 128],
      "aus:vic:vic-02-FKN:": [0, 128, 0],
      "aus:vic:vic-02-STY:": [0, 128, 0],
      "aus:vic:vic-01-ART:": [191, 0, 255],
      "aus:vic:vic-01-BAT:": [191, 0, 255],
      "aus:vic:vic-01-BDE:": [191, 0, 255],
      "aus:vic:vic-01-BGO:": [191, 0, 255],
      "aus:vic:vic-01-ECH:": [191, 0, 255],
      "aus:vic:vic-01-GEL:": [191, 0, 255],
      "aus:vic:vic-01-MBY:": [191, 0, 255],
      "aus:vic:vic-01-SER:": [191, 0, 255],
      "aus:vic:vic-01-SNH:": [191, 0, 255],
      "aus:vic:vic-01-SWL:": [191, 0, 255],
      "aus:vic:vic-01-TRN:": [191, 0, 255],
      "aus:vic:vic-01-WBL:": [191, 0, 255]
    }
  }
}
